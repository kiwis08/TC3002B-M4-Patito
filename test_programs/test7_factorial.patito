program test7_factorial;
var n, result: int;

int factorial(x: int)[ var acc, i, limit: int; ] {
    acc = 1;
    i = 1;
    limit = x + 1;
    while (i < limit) do {
        acc = acc * i;
        i = i + 1;
    };
    return acc;
};

main {
    n = 5;
    result = factorial(n);
    print("factorial(5) = ", result);
}
end


